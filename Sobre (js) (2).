                        return n.a.post("/approved-payment", {
                            idPayment: e,
                            type: t,
                            userId: a
                        })
                    },
                    rejectedStatus: function(e, t) {
                        return n.a.post("/rejected-payment", {
                            idPayment: e,
                            userId: t
                        })
                    },
